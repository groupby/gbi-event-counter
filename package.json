{
  "name": "gbi-event-counter",
  "version": "0.1.15-dev",
  "description": "gbi-event-counter",
  "keywords": [
    "groupby",
    "tracker"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/groupby/gbi-event-counter.git"
  },
  "author": "GroupBy Inc.",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/gbi-event-counter-0.1.15-dev.js",
  "types": "dist/gbi-event-counter-0.1.15-dev.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist cdn 2> /dev/null",
    "increment-version": "node scripts/node/increment-version.cjs",
    "build:cdn": "tsc && vite build --config vite.config.cdn.ts",
    "build:npm": "tsc && vite build --config vite.config.npm.ts",
    "dev": "vite --config vite.config.dev.ts",
    "lint": "eslint .",
    "test:dev": "vitest --run --config vite.config.dev.ts",
    "test:cdn": "vitest --run --config vite.config.cdn.ts",
    "test:npm": "vitest --run --config vite.config.npm.ts",
    "format": "prettier --write .",
    "prepare": "node .husky/install.mjs"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/js": "^9.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.5.0"
  }
}
